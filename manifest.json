{
  "manifest_version": 2,

  "name": "Sending love to 오빠",
  "description": "This extension will sendmail to 오빠",
  "version": "1.0",
  "browser_action": {
   "default_icon": "ttd.png",
   "default_popup": "index.html"
  },
  "content_scripts": [
    {
        "matches": ["http://*/*, http:s//*/*"],
        "js" : ["jquery-3.5.1.min.js", "index.js", "bootstrap.min.js"]
    }
  ],
  "permissions": [
   "activeTab"
   ]
}
